{
  "name": "fabric-layers-core",
  "version": "1.1.9",
  "description": "A fabric.js coordinate-plane (grid) & layers library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "browser": {
    "./dist/index.cjs.js": "./dist/index.umd.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --require @babel/register --require ./test/setup.js",
    "build": "npm run clean && rollup -c rollup.config.mjs",
    "build:watch": "rollup -c rollup.config.mjs -w",
    "start": "webpack serve --mode=development --open",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoslin103/fabric-layers-core.git"
  },
  "keywords": [
    "fabric.js",
    "canvas",
    "grid",
    "coordinate-plane",
    "layers",
    "axis",
    "polar",
    "cartesian"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajoslin103/fabric-layers-core/issues"
  },
  "homepage": "https://github.com/ajoslin103/fabric-layers-core#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.24.5",
    "@babel/runtime": "^7.24.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.10",
    "conventional-changelog-cli": "^2.2.2",
    "core-js": "^3.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "standard-version": "^9.5.0",
    "webpack": "^5.89.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "eventemitter2": "^6.4.9",
    "fabric": "^5.5.2-browser"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
